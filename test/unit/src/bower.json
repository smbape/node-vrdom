{
  "name": "vrdom-test",
  "version": "0.0.0",
  "main": "**",
  "umd": true,
  "ignore": [
    "**/.*",
    "component.json",
    "package.json",
    "node_modules"
  ],
  "licenses": "WTFPL",
  "dependencies": {
    "bootstrap": "^3.3.7",
    "jquery": "^3.1.1",
    "lodash": "^4.17.4",
    "material-design-lite": "^1.3.0",
    "preact": "^7.2.0",
    "preact-compat": "^3.13.1",
    "proptypes": "^0.14.4",
    "qs": "git+https://github.com/hapijs/qs#^6.2.1",
    "react": "^15.4.2"
  },
  "overrides": {
    "backbone": {
      "dependencies": {
        "jquery": "*",
        "lodash": "*"
      }
    },
    "bootstrap": {
      "main": [
        "dist/css/bootstrap.css",
        "dist/js/bootstrap.js",
        "dist/fonts/**"
      ],
      "dependencies": {
        "jquery": "*"
      },
      "exports": "jQuery.fn.emulateTransitionEnd"
    },
    "lodash": {
      "main": "dist/lodash.js"
    },
    "material-design-lite": {
      "main": [
        "material.js",
        "material.css"
      ],
      "exports": "componentHandler"
    },
    "react": {
      "main": [
        "react.min.js"
      ],
      "map": {
        "react-dom.min.js": "react-dom"
      }
    },
    "vrdom": {
      "main": [
        "dist/vrdom.js"
      ],
      "map": {
        "dist/vrdom-compat.js": "vrdom-compat"
      }
    }
  },
  "resolutions": {
    "jquery": "^3.1.0"
  }
}